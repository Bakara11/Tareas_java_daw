package Clases;

public class Principal {

	public static void main(String[] args) {
		
		Profesor profesor1 = new Profesor("516542981R", "Alfredo di Estefano", "C/ cuatro gatos","666459822","Educacion Fisica");
		Alumno alumno1 = new Alumno("136524422L", "Enrique Fernandez", "C/ Sol Iluminador", "6392397899", "2º Bachillerato");
		Administrativo administrativo1 = new Administrativo("432154731A", "Pablo Segura", "C/ Canarias 23 ", "634582199", "Hacer matriculas, controlar asistencia");
		
		System.out.println(profesor1.toString());
		profesor1.trabajar();
		System.out.println("\n");
		System.out.println(alumno1.toString());
		alumno1.trabajar();
		System.out.println("\n");
		System.out.println(administrativo1.toString());
		administrativo1.trabajar();
		System.out.println("\n");
		System.out.println(alumno1.llamar(profesor1));
		System.out.println("\n");
		System.out.println(profesor1.ponerNotas());
		System.out.println(alumno1.hacerExamen());
		System.out.println(administrativo1.gestionarMatricula());
		
		
		
	}

}
